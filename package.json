{
  "name": "@jesses/ts-docker-hub",
  "description": "A minimal, type-safe API for querying top repos / total pulls for a user from Docker Hub.",
  "version": "0.0.1",
  "author": "Jesse Stuart <hi@jessestuart.com>",
  "bugs": {
    "url": "https://github.com/jessestuart/ts-docker-hub/issues"
  },
  "dependencies": {
    "@babel/runtime": "7.5.5",
    "async": "2.6.3",
    "aws-sdk": "2.493.0",
    "axios": "0.19.0",
    "lodash": "4.17.13"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "7.5.4",
    "@babel/plugin-transform-runtime": "7.5.0",
    "@babel/preset-env": "7.5.4",
    "@babel/preset-typescript": "7.3.3",
    "@semantic-release/git": "7.0.16",
    "@types/async": "3.0.0",
    "@types/axios": "0.14.0",
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.136",
    "@types/luxon": "1.15.2",
    "@types/node": "12.6.2",
    "@types/pino": "5.8.8",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "babel-jest": "24.8.0",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-lodash": "3.3.4",
    "babel-preset-minify": "0.5.0",
    "codacy-coverage": "3.4.0",
    "codecov": "3.5.0",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.2.1",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.0",
    "jest": "24.8.0",
    "jest-junit": "6.4.0",
    "mocha": "6.1.4",
    "nodemon": "1.19.1",
    "pino": "5.12.6",
    "prettier": "1.18.2",
    "prettier-eslint": "9.0.0",
    "prettier-tslint": "0.4.2",
    "semantic-release": "15.13.18",
    "sinon": "7.3.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.5.3"
  },
  "homepage": "https://github.com/jessestuart/ts-docker-hub",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jessestuart/ts-docker-hub.git"
  },
  "scripts": {
    "build": "babel --extensions '.ts' src/ --out-dir dist/",
    "check-types": "tsc",
    "dev": "ts-node ./src/index.ts",
    "lint": "eslint src/** --ext ts,tsx",
    "prepublishOnly": "yarn build && yarn ts:defs && prettier --write index.d.ts",
    "release": "yarn build && semantic-release",
    "test": "concurrently 'yarn check-types' 'yarn jest --coverage'",
    "test:watch": "concurrently 'yarn tsc --watch' 'yarn jest --watch'",
    "ts:defs": "tsc --declaration --emitDeclarationOnly",
    "update": "ncu -ua && yarn -s",
    "watch": "yarn build -w"
  }
}
